{
    "final_space": false,
    "console_title": false,
    "console_title_style": "",
    "console_title_template": "",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "aws",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#26C6DA",
                    "background": "#546E7A",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {                        
						"template": "{{.Profile}}{{if .Region}}\uf52c{{.Region}}{{end}}"
                    }
                },
                {
                    "type": "root",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#26C6DA",
                    "background": "#546E7A",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": null
                },
                {
                    "type": "path",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#26C6DA",
                    "background": "#546E7A",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "postfix": " ",
                        "style": "full"
                    }
                },
                {
                    "type": "git",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#D4E157",
                    "background": "#546E7A",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": "\u003c#26C6DA\u003e \u003c/\u003e"
                    }
                },
                {
                    "type": "text",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#546E7A",
                    "background": "transparent",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "postfix": "",
                        "prefix": "",
                        "text": ""
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "session",
                    "style": "diamond",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#D4E157",
                    "background": "#546E7A",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "postfix": " \u003c#26C6DA\u003e\u003c/\u003e "
                    }
                },
                {
                    "type": "time",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#D4E157",
                    "background": "#546E7A",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "postfix": "  ",
                        "time_format": "15:04:05"
                    }
                }
            ]
        },
        {
            "type": "newline",
            "alignment": "",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": null
        },
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "exit",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#D4E157",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "always_enabled": true,
                        "display_exit_code": false,
                        "error_color": "#FF5252",
                        "prefix": "❯"
                    }
                }
            ]
        }
    ]
}
